num1 = float(input("Digite um número: "))

num2 = float(input("Digite o numero 2: "))

num3=float(input("Digite o numero 3: "))

num4=float (input("Digite o numero 4: "))


media = num1 + num2 + num3 + num4

total = media / 4

arredondado = round(total, 2)

print("A média de números é:", arredondado)
